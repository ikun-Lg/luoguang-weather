.tody{position:relative;height:318rpx;width:650rpx;margin:20rpx auto}.tody .title{display:flex;justify-content:space-between;align-items:center;height:44rpx}.tody .title .leftItem{font-size:40rpx;font-weight:600;line-height:44rpx;color:#353535}.tody .title .rightItem{font-size:24rpx;font-weight:500;line-height:44rpx;color:#6652f1}.tody .title .rightItem:active{color:#3737f1}.tody .other{position:absolute;width:650rpx;height:244rpx;margin-top:30rpx;border-radius:40rpx;border:2rpx solid #fff;background:#fff;display:flex;justify-content:space-around;align-items:center}.tody .other .otherItem{display:flex;flex-direction:column;justify-content:space-evenly;align-items:center;width:148rpx;height:204rpx;border-radius:32rpx;background:linear-gradient(208.92deg,rgba(102,82,241,.53),#6652f1)}.tody .other .otherItem .icon{width:148rpx;height:106rpx}.tody .other .otherItem .num{font-size:26rpx;font-weight:600;color:#fff;text-align:center;vertical-align:middle}.tody .other .otherItem .iconText{font-size:14rpx;font-weight:600;color:hsla(0,0%,100%,.62);text-align:center;vertical-align:middle}.tody .other .blur{position:absolute;left:34rpx;top:80rpx;width:580rpx;height:226rpx;opacity:1;border-radius:40rpx;background:rgba(102,82,241,.3);-webkit-filter:blur(120rpx);filter:blur(120rpx)}